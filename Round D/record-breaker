//Solution of Problem A Record Breaker from Kick Start 2020
#include<iostream>
#include<stack>
using namespace std;
int main()
{
    int t,n,c,m;
    cin>>t;
    for(int k=1;k<=t;k++)
    {
        c=0;m=-1000000000;
        stack<int>s;
        cin>>n;
        int a[n];
        for(int i=0;i<n;i++)
        {
            cin>>a[i];
        }
        for(int i=0;i<n;i++)
        {
            if(a[i]>m)
            {
                m=a[i];
                if(a[i]>a[i+1] && i!=(n-1))
                c++;
                else if(i==(n-1))
                {
                    c++;
                }
            }
        }
        cout<<"Case #"<<k<<":"<<" "<<c<<"\n";
    }
    return 0;
}
